CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
ENABLE_BITCODE = NO
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(SDKROOT)/usr/include/libxml2 $(SDKROOT)/usr/include/libxml2 $(SDKROOT)/usr/include/libresolv $(SDKROOT)/usr/include/libiconv
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/MMXXMPPFramework/Vendor/libidn"
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/AFAmazonS3Manager.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/AFNetworking.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Bolts.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/CocoaAsyncSocket.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/CocoaLumberjack.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/FBSDKCoreKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/FBSDKLoginKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/JSQMessagesViewController.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/JSQSystemSoundPlayer.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/MDMCoreData.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/MMX.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/MMXXMPPFramework.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Mantle.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AFAmazonS3Manager" -framework "AFNetworking" -framework "Bolts" -framework "CocoaAsyncSocket" -framework "CocoaLumberjack" -framework "FBSDKCoreKit" -framework "FBSDKLoginKit" -framework "JSQMessagesViewController" -framework "JSQSystemSoundPlayer" -framework "MDMCoreData" -framework "MMX" -framework "MMXXMPPFramework" -framework "Mantle"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Hola
PODS_ROOT = ${SRCROOT}/Pods